<%- include("partials/head", {title : "movie-see"}) %>
<div class="container">
    <%- include("partials/navbar") %>
    <h1><%= data.name %></h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="supplierName">supplierName</th>
                <th scope="address">address</th>
                <th scope="contactNumber">Contsct Number</th>
                <th scope="email">Email</th>
            </tr>
        </thead>
        <tbody>
            <% data.Suppliers.forEach(e => { %>
            <tr>
                <td><%= e.supplierName %></td>
                <td><%= e.address %></td>
                <td><%= e.contactNumber%></td>
                <td><%= e.email %></td>
            </tr>
            <%})%>
        </tbody>

    </table>
</div>
<%- include("partials/foot") %>